<h2>Channel</h2>

<p id="notice"><%= notice %></p>

<% if ! @record.nil? %>
  <table id='channel_info' class="display display_only">
    <thead>
      <tr>
        <th align="left">Field</th>
        <th align="left">Value</th>
      </tr>
    </thead>

    <tbody>
      <tr><td>ID</td><td><%= @record["id"] %></td></tr>
      <tr><td>Name</td><td><%= @record["name"] %></td></tr>
      <tr><td>Timezone</td><td><%= @record["timezone"] %></td></tr>
    </tbody>
  </table>

  <br/>

  <%= link_to 'Edit', action: 'edit', id: @record["id"] %>
<% end %>
