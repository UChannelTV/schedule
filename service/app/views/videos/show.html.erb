<h2>Video</h2>

<p id="notice"><%= notice %></p>

<% if ! @record.nil? %>
  <table id='video_info' class="display display_only">
    <thead>
      <tr>
        <th align="left">Field</th>
        <th align="left">Value</th>
      </tr>
    </thead>

    <tbody>
      <tr><td>ID</td><td><%= @record["id"] %></td></tr>
      <tr><td>Path</td><td><%= @record["path"] %></td></tr>
      <tr><td>Format</td><td><%= @record["format"] %></td></tr>
      <tr><td>Size</td><td><%= @record["size"] %></td></tr>
      <tr><td>Duration</td><td><%= @record["duration"] %></td></tr>
      <tr><td>Mux Bitrate</td><td><%= @record["mux_bitrate"] %></td></tr>
      <tr><td>Variable Mux Rate</td><td><%= @record["variable_mux_rate"] %></td></tr>
      <tr><td>Video Codec</td><td><%= @record["video_codec"] %></td></tr>
      <tr><td>Video Bitrate</td><td><%= @record["video_bitrate"] %></td></tr>
      <tr><td>Frame Rate</td><td><%= @record["frame_rate"] %></td></tr>
      <tr><td>Video Height</td><td><%= @record["video_height"] %></td></tr>
      <tr><td>Video Width</td><td><%= @record["video_width"] %></td></tr>
      <tr><td>Audio Codec</td><td><%= @record["audio_codec"] %></td></tr>
      <tr><td>Audio Bitrate</td><td><%= @record["audio_bitrate"] %></td></tr>
      <tr><td>Audio Sample Rate</td><td><%= @record["audio_sample_rate"] %></td></tr>
      <tr><td>Status</td><td><%= @record["status"] %></td></tr>
      <tr><td>Remark</td><td><%= @record["remark"] %></td></tr>
      <tr><td>Created At</td><td><%= @record["created_at"] %></td></tr>
      <tr><td>Updated At</td><td><%= @record["updated_at"] %></td></tr>
    </tbody>
  </table>

  <br/>

  <%= link_to 'Edit', action: 'edit', id: @record["id"] %>
<% end %>
