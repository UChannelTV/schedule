<h2>Channel Schedule Version</h2>

<p id="notice"><%= notice %></p>

<% if ! @record.nil? %>
  <table id='channel_schedule_version_info' class="display display_only">
    <thead>
      <tr>
        <th align="left">Field</th>
        <th align="left">Value</th>
      </tr>
    </thead>

    <tbody>
      <tr><td>ID</td><td><%= @record["id"] %></td></tr>
      <tr><td>Channel ID</td><td><%= @record["channel_id"] %></td></tr>
      <tr><td>Active Day</td><td><%= @record["active_day"] %></td></tr>
    </tbody>
  </table>

  <br/>

  <%= link_to 'Edit', action: 'edit', id: @record["id"] %>
<% end %>
